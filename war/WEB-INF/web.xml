<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>BlogHomework3</servlet-name>
		<servlet-class>com.ee461l.HomeworkBlog.BlogHomework3Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BlogHomework3</servlet-name>
		<url-pattern>/bloghomework3</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>createPost</servlet-name>
		<servlet-class>com.ee461l.HomeworkBlog.CreateBlogPostServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createPost</servlet-name>
		<url-pattern>/createPost</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>viewPosts</servlet-name>
		<servlet-class>com.ee461l.HomeworkBlog.ViewAllPostsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>viewPosts</servlet-name>
		<url-pattern>/showAllPosts</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>userSubscribe</servlet-name>
		<servlet-class>com.ee461l.HomeworkBlog.UserSubscribe</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>userSubscribe</servlet-name>
		<url-pattern>/subscribeUser</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>userUnsubscribe</servlet-name>
		<servlet-class>com.ee461l.HomeworkBlog.UserUnsubscribe</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>userUnsubscribe</servlet-name>
		<url-pattern>/unsubscribeUser</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>updateSubscribers</servlet-name>
		<servlet-class>com.ee461l.HomeworkBlog.UpdateSubscribers</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateSubscribers</servlet-name>
		<url-pattern>/cron/subscriberUpdate</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
